{
	"useTabs": true,
	"trailingComma": "none",
	"tabWidth": 4,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"htmlWhitespaceSensitivity": "ignore",
	"printWidth": 120,
	"proseWrap": "preserve",
	"overrides": [
		{
			"files": "*.ts*",
			"options": {
				"useTabs": false,
				"tabWidth": 2,
				"printWidth": 120
			}
		}
	]
}
